                echo "In Build stage"
            }
        }
        stage('Test') {
            steps {
                echo "In Test stage"
            }
        }
        stage('Deploy') {
            steps {
                echo "In Deploy stage"
            }
        }
        stage('Archive') {
            steps {
                echo "In Archive step "
            }
        }
    }
}
